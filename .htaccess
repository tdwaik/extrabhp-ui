Header set Cache-Control "max-age=0, no-cache"

Options +FollowSymLinks
Options -Indexes

RewriteEngine on

RewriteCond %{HTTP_HOST} ^(?!dev).*\.extrabhp\.com [NC]
RewriteRule ^(.*) http://extrabhp.com [R=301,L,QSA]

RewriteCond %{REQUEST_URI} ^\/less\/.*|^\/app\/.*|^\/\..*|^\/gulpfile.js|^\/package.json|^\/package-lock.json
RewriteRule ^(.+) / [R=404,L]

ErrorDocument 404 /
